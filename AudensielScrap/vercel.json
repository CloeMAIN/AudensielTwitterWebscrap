{
    "builds": [{
        "src": "AudensielScrap/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
    }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "AudensielScrap/wsgi.py"
        }
    ],
    "functions": {
        "install-playwright": {
            "runtime": "python3.9",
            "includeFiles": "AudensielScrap/AudensielScrap/install_playwright_script.py"
          }
        },
    "alias": "optimisation-scraping"
}
